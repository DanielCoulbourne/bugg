{
    "name": "kathunk/bugg",
    "description": "Thunk Debugging Utilities",
    "keywords": [
        "kathunk",
        "bugg",
        "php"
    ],
    "homepage": "https://github.com/kathunk/bugg",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Coulbourne",
            "email": "daniel.coulbourne@gmail.com",
            "homepage": "https://thunk.devm",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tighten\\Bugg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tighten\\Bugg\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tighten\\Bugg\\BuggServiceProvider"
            ],
            "aliases": {
                "Bugg": "Tighten\\Bugg\\BuggFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
